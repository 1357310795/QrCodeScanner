    <DockPanel LastChildFill="True">
        <DockPanel Margin="15,15,15,10"
                   LastChildFill="false" DockPanel.Dock="Top">
            <Border Width="4" Height="22"
                    Background="{DynamicResource PrimaryHueMidBrush}"
                    DockPanel.Dock="Left" />
            <TextBlock Margin="10,0" FontWeight="Bold"
                       Text="{Binding CodeType}"
                       DockPanel.Dock="Left"
                       FontSize="22" />
        </DockPanel>
        <Grid x:Name="grid1"
              Margin="10,0,0,0"
              DockPanel.Dock="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Button x:Name="ButtonOpenUri"
                    Height="80"
                    Margin="0,10,10,10" Padding="1"
                    Click="ButtonGo_Click"
                    Style="{StaticResource MaterialDesignOutlinedButton}">
                <StackPanel Orientation="Vertical">
                    <materialDesign:PackIcon Width="32" Height="32"
                                             HorizontalAlignment="Center"
                                             Margin="0,0,0,5"
                                             Kind="ArrowRightCircleOutline" />
                    <TextBlock HorizontalAlignment="Center" FontSize="18" Text="{DynamicResource OpenUri}">
                        
                    </TextBlock>
                </StackPanel>
            </Button>
            <Button x:Name="ButtonCopy"
                    Height="80"
                    Margin="0,10,10,10" Padding="1"
                    Grid.Column="1"
                    Click="ButtonCopy_Click"
                    Style="{StaticResource MaterialDesignOutlinedButton}">
                <StackPanel Orientation="Vertical">
                    <materialDesign:PackIcon Width="32" Height="32"
                                             HorizontalAlignment="Center"
                                             Margin="0,0,0,5"
                                             Kind="ContentCopy" />
                    <TextBlock x:Name="ButtonCopyText"
                               HorizontalAlignment="Center"
                               FontSize="18" Text="{DynamicResource Copy}">
                        
                    </TextBlock>
                </StackPanel>
            </Button>
            <Button x:Name="ButtonContinue"
                    Height="80"
                    Margin="0,10,10,10" Padding="1"
                    Grid.Column="2"
                    Click="ButtonContinue_Click"
                    Style="{StaticResource MaterialDesignOutlinedButton}">
                <StackPanel Orientation="Vertical">
                    <materialDesign:PackIcon Width="32" Height="32"
                                             HorizontalAlignment="Center"
                                             Margin="0,0,0,5"
                                             Kind="QrcodeScan" />
                    <TextBlock HorizontalAlignment="Center" FontSize="18" Text="{DynamicResource ContinueScan}">
                        
                    </TextBlock>
                </StackPanel>
            </Button>
        </Grid>
        <TextBox x:Name="text1"
                 TextWrapping="Wrap" Margin="15,0,15,0"
                 Text="{Binding Data}"
                 AcceptsReturn="True" HorizontalScrollBarVisibility="Auto"
                 BorderThickness="0" AutoWordSelection="True"
                 IsReadOnly="True" IsUndoEnabled="False"
                 FontSize="16" />
    </DockPanel>
</Window>
